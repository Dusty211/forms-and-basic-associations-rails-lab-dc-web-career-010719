<h1>Create a Song</h1>
<div>

<%= form_for @song  do |f| %><br>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %><br>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br>
  Notes 1:<input name='song[note_contents][]' id='song_notes_1'><br>
  Notes 2:<input name='song[note_contents][]' id='song_notes_2'><br>
  Notes 3:<input name='song[note_contents][]' id='song_notes_3'><br>


  <%= f.submit %><br>
<% end %>
</div>
